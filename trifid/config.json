{
  "baseConfig": "trifid:config-base.json",
  "handler": {
    "root": {
      "module": "trifid-handler-fetch",
      "options": {
        "resource": "https://cube.link/",
        "url": "cwd:vocab.ttl",
        "contentType": "text/turtle",
        "cache": "true"
      }
    },
    "view": {
      "path": "/view/",
      "module": "trifid-handler-fetch",
      "options": {
        "resource": "https://cube.link/view/",
        "url": "cwd:vocab.ttl",
        "contentType": "text/turtle",
        "cache": "true"
      }
    }
  },
  "staticViews": {
    "/": null
  },
  "plugins": {
    "vocab-redirect": {
      "priority": 55,
      "module": "trifid-core:./plugins/middleware",
      "middleware": "cwd:trifid/redirect"
    }
  }
}
